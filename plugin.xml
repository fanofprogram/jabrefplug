<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="Gmail Importer" version="1.0">
	
	<requires>
	  <import plugin-id="net.sf.jabref.core"/>
	</requires>
	
	<!-- Will load code and resources from the home folder -->
	<runtime>
	  <library type="code" path="./" id="code"/>
	  <library type="resources" path="./" id="resouces"/>
	</runtime>
	
	<extension id="GmailImporterPane" plugin-id="net.sf.jabref.core"
		   point-id="SidePanePlugin">
	  <parameter id="sidePanePlugin"
		     value="skyeagle.plugin.gui.GmailImporterPane" />
	  <parameter id="name" value="Gmail Importer" />
	  <parameter id="description"
		     value="Gmail import reference" />
	</extension>
	
</plugin>
